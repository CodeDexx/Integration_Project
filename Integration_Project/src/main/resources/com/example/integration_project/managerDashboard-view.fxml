<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="432.0" prefWidth="486.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.integration_project.Controller.ManagerDashboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Pane prefHeight="413.0" prefWidth="476.0">
         <children>
            <Label layoutX="134.0" layoutY="14.0" prefHeight="38.0" prefWidth="115.0" text="Manager Dashboard" />
            <Button fx:id="aLogoutButton" layoutX="380.0" layoutY="14.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" style="-fx-background-color: red;" text="Logout" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button layoutX="355.0" layoutY="94.0" mnemonicParsing="false" onAction="#onMoviesButtonClick" prefHeight="25.0" prefWidth="83.0" style="-fx-background-color: blue;" text="Movies" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button layoutX="354.0" layoutY="124.0" mnemonicParsing="false" onAction="#onShowtimeButtonClick" prefHeight="25.0" prefWidth="83.0" style="-fx-background-color: blue;" text="Showtimes" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button layoutX="355.0" layoutY="156.0" mnemonicParsing="false" onAction="#onShowroomsButtonClick" prefHeight="25.0" prefWidth="83.0" style="-fx-background-color: blue;" text="Showrooms" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Label fx:id="aPageTitle" layoutX="72.0" layoutY="52.0" prefHeight="31.0" prefWidth="240.0" text="Movies/Showtime/Showroom">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
            <ListView fx:id="aListView" layoutX="39.0" layoutY="94.0" prefHeight="186.0" prefWidth="303.0" />
            <Button layoutX="63.0" layoutY="304.0" mnemonicParsing="false" onAction="#onAddButtonClick" prefHeight="25.0" prefWidth="52.0" style="-fx-background-color: blue;" text="Add" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button layoutX="142.0" layoutY="304.0" mnemonicParsing="false" onAction="#onUpdateButtonClick" prefHeight="25.0" prefWidth="69.0" style="-fx-background-color: blue;" text="Update" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button layoutX="232.0" layoutY="304.0" mnemonicParsing="false" onAction="#onDeleteButtonClick" prefHeight="25.0" prefWidth="60.0" style="-fx-background-color: blue;" text="Delete" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button layoutX="355.0" layoutY="187.0" mnemonicParsing="false" onAction="#onTicketsButtonClick" prefHeight="25.0" prefWidth="83.0" style="-fx-background-color: blue;" text="Tickets" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</VBox>
